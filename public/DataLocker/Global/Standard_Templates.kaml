<!-- Core styling -->
<style>
  input {
    text-align: left;
  }

  div.form-control-display-only {
    font-weight: bold;
  }

  /* ASP.NET listcontrols render in a table */
  .katapp-css table.bs-listcontrol tr, .katapp-css table.bs-listcontrol td {
      border-width: 0; /* in case nested in another table */
      background-color: transparent !important;
      padding: 2px;
  }
  
      /* Checkbox and Radio styling */
      .katapp-css .abc-checkbox label,
      .katapp-css .checkbox label,
      .katapp-css .radio label,
      .katapp-css .abc-radio label {
          /*  BS has positioning CSS on input to help when label wraps, but wasn't exactly what we wanted, 
              so this accomplished keeping label and input together when wraps */
          display: inline; 
      }

          /*  Awesome checkbox has some spacing and styling when disabled, but not requiring that dependency */
          .katapp-css table.bs-listcontrol.abc-radio label,
          .katapp-css table.bs-listcontrol.abc-checkbox label,
          .katapp-css table.bs-listcontrol.radio label,
          .katapp-css table.bs-listcontrol.checkbox label,
          .katapp-css .form-inline .abc-radio label,
          .katapp-css .form-inline .abc-checkbox label,
          .katapp-css .form-inline .radio label,
          .katapp-css .form-inline .checkbox label {
              padding-left: 5px;
          }
      
          .katapp-css .radio input[type="radio"]:disabled,
          .katapp-css .abc-radio input[type="radio"]:disabled,
          .katapp-css .radio input[type="radio"]:disabled+label::before,
          .katapp-css .abc-radio input[type="radio"]:disabled+label::before {
              cursor: not-allowed
          }
          .katapp-css .radio input[type="radio"]:disabled+label,
          .katapp-css .abc-radio input[type="radio"]:disabled+label {
              opacity: .65
          }

          /* For horizontal lists, have to apply some styling to make it look better */
          .katapp-css .form-inline .form-group .abc-checkbox label::before,
          .katapp-css .form-inline .form-group .abc-checkbox label::after,
          .katapp-css .form-inline .form-group .abc-radio label::before,
          .katapp-css .form-inline .form-group .abc-radio label::after,
          .katapp-css .form-inline .form-group .checkbox label::before,
          .katapp-css .form-inline .form-group .checkbox label::after,
          .katapp-css .form-inline .form-group .radio label::before,
          .katapp-css .form-inline .form-group .radio label::after {
              margin-left: -25px;
          }

          /* Color input border */
          /*
          .katapp-css .abc-radio label:before,
          .katapp-css .abc-checkbox label:before,
          .katapp-css .radio label:before,
          .katapp-css .checkbox label:before {
            border: 1px solid #333;
          }
          */

          .katapp-css .abc-radio table.form-control,
          .katapp-css .abc-checkbox table.form-control,
          .katapp-css .abc-radio span.form-control,
          .katapp-css .abc-checkbox span.form-control,
          .katapp-css .radio table.form-control,
          .katapp-css .checkbox table.form-control,
          .katapp-css .radio span.form-control,
          .katapp-css .checkbox span.form-control {
              border-width: 0;
              box-shadow: 0 0 0 0;
              height: auto;
              padding: 0 12px 0 12px;
          }
          .katapp-css .form-inline.list-container .radio,
          .katapp-css .form-inline.list-container .abc-radio {
              margin: 5px 20px 5px 2px;
          }
          .katapp-css .form-inline.list-container {
              padding-left: 0;
          }
          .katapp-css .form-inline.list-container .abc-radio label,
          .katapp-css .form-inline.list-container .abc-checkbox label,
          .katapp-css .form-inline.list-container .radio label,
          .katapp-css .form-inline.list-container .checkbox label {
              padding-left: 5px;
          }

  /* Ajax blocker during calculations */
  .katapp-css .ajaxContent {
      position: relative;
      min-height: 200px;
  }
  .katapp-css .ajaxloader {
      position: absolute;
      top: -1px;
      left: -1px;
      bottom: -1px;
      right: -1px;
      color: #000;
      background-size: 30px;
      z-index: 100;
      opacity: .8;
      background: #fff url('data:image/gif;base64,') center center no-repeat;
  }
  
  /* noUIslider */
  .katapp-css .noUi-handle {
        box-shadow: none;
        background: rgb(0 124 188);
        border-width: 0;
        cursor: pointer;
        top: -8px;
        width: 20px;
        height: 20px;
        border-radius: 50%;
    }
  
    .katapp-css .noUi-handle:focus {
        outline: none;
    }
  
    .katapp-css .noUi-target {
        background: lightblue;
        height: 5px;
    }
  
    .katapp-css .noUi-handle:after,
    .katapp-css .noUi-handle:before {
        content: none;
    }
  
  
    .katapp-css .slider-value {
        font-weight: bold;
    }
  
    .katapp-css .noUi-target {
        border-radius: 0;
        box-shadow: none;
        border: none;
    }
  
    .katapp-css .noUi-connect,
    .katapp-css [disabled].noUi-connect {
        background-color: #007CBC;
        border: none;
        box-shadow: none;
    }
  
    .katapp-css div[disabled] .noUi-handle {
        background: #e7e7e7;
    }
  
    .katapp-css .noUi-handle div {
        padding-top: 23px;
        font-size: 11px;
        text-align: center;
    }
  
    .katapp-css .noUi-pips {
        font-weight: normal;
        font-family: inherit;
        font-size: 11px;
        color: #333333;
    }
  
    .katapp-css .noUi-pips-horizontal {
        height: auto;
    }
  
    .katapp-css .slider-control span {
        display: none;
    }
  
  /* Input Validation Display */
  .katapp-css .validator-container {
      position: relative;
  }
  
  .katapp-css .validator-container.error .error-msg,
  .katapp-css .validator-container.warning .error-msg {
          display: inline-block;
          z-index: 3; /* with out this, inputs with group addons before input didn't show tooltip for some reason */
      }
  
      .katapp-css .slider-container.error,
      .katapp-css .checkbox-container.error,
      .katapp-css .validator-container.error .form-control {
          background-image: url(data:image/png;base64,);
          background-repeat: no-repeat;
          background-position: right center;
          border-color: red;
          border-width: 1px;
      }
  
    .katapp-css .slider-container.error {
      background-position: right top;
  }
  
  .katapp-css .slider-container.warning,
  .katapp-css .checkbox-container.warning,
  .katapp-css .validator-container.warning .form-control {
      background-image: url(data:image/png;base64,);
      background-repeat: no-repeat;
      background-position: right center;
      border-color: #FCF8E3;
      border-width: 1px;
  }
  
  .katapp-css .slider-container.warning {
      background-position: right top;
  }
  
          .katapp-css .validator-container.warning .form-control.text-right,
          .katapp-css .validator-container.error .form-control.text-right,
          .katapp-css .validator-container.error .spinner .form-control,
          .katapp-css .validator-container.warning .spinner .form-control {
              padding-right: 35px
          }
  
      .katapp-css .checkbox-container.validator-container.error,
      .katapp-css .checkbox-container.validator-container.warning {
        border: 1px solid red;
        border-radius: 4px;
      }
  
  .katapp-css .checkbox-container.validator-container.warning {
      border-color: #FCF8E3;
  }
  
  .katapp-css .list-container.validator-container.error,
  .katapp-css .list-container.validator-container.warning {
      border: 1px solid red;
      border-radius: 4px;
      background-position: right top;
  }
  .katapp-css .list-container.validator-container.warning {
      border-color: #FCF8E3;
  }
  
  .katapp-css .validator-container .tooltip,
  .katapp-css .tooltip.error,
  .katapp-css.tooltip.error /* append to body by BS so katapp-css is dual class */ {
      min-width: 140px;
  }
  
  .katapp-css .validator-container .tooltip.in,
  .katapp-css .tooltip.error.in,
  .katapp-css .validator-container .tooltip.show,
  .katapp-css .tooltip.error.show,  
  .katapp-css.tooltip.error.in, .katapp-css.tooltip.error.show /* append to body by BS so katapp-css is dual class */ {
      opacity: 1;
  }
  
      .katapp-css .validator-container.error .tooltip-inner,
      .katapp-css .validator-container.warning .tooltip-inner,
      .katapp-css .tooltip.error .tooltip-inner,
      .katapp-css .tooltip.warning .tooltip-inner,
      .katapp-css.tooltip.error .tooltip-inner, .katapp-css.tooltip.warning .tooltip-inner /* append to body by BS so katapp-css is dual class */ {
          padding: 8px;
          background-color: #f8d7da;
          border: solid #721c24 1px;
          color: #721c24;
          text-align: left;
      }
  
  .katapp-css .validator-container.warning .tooltip-inner,
  .katapp-css .tooltip.warning .tooltip-inner,
  .katapp-css.tooltip.warning .tooltip-inner /* append to body by BS so katapp-css is dual class */ {
      background-color: #FCF8E3;
      color: #8A6D3B;
  }
  
  /* BS 4 */
  .katapp-css.error.bs-tooltip-auto[x-placement^=bottom] .arrow::before,
  .katapp-css.error.bs-tooltip-bottom .arrow::before {
      border-bottom-color: #721c24;
  }
  .katapp-css.error.bs-tooltip-auto[x-placement^=left] .arrow::before,
