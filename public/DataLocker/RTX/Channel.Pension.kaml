<script>
easyScrollDots({
  'fixedNav': false,
  'fixedNavId': '',
  'fixedNavUpward': false
});
//ensure header included file loaded first before the containing functions can be use
$loadedMainScript.done(function() {
	
	$(function () {
		$('.hero').fadeIn('slow');
		$('#carousel-example-generic').carousel({
			interval:5000,
			pause: "false"
		});
		
	});
		
	$(".carausalbutton").click(function () {
		if ($(this).attr("id") === "pauseButton") {
		  $('#carousel-example-generic').carousel('pause');
				$(this).attr("id", "playButton");
		  $("span",this).toggleClass("glyphicons-play glyphicons-pause");
		} else {
				  $('#carousel-example-generic').carousel('cycle');
				$(this).attr("id", "pauseButton");
		  $("span",this).toggleClass("glyphicons-pause glyphicons-play");
		}
	});
					
	$('.carousel').carousel({
	  interval: 5000
	})

	$('.401kSavingBtn').collapse('show').css("width", "100%");
	
	var swiper = new Swiper('.swiper-container', {
	  slidesPerView: 1,
	  spaceBetween: 10,
	  // init: false,
	  pagination: {
		el: '.swiper-pagination',
		clickable: true,
	  },
	  breakpoints: {
		'@0.00': {
		  slidesPerView: 1.5,
		  spaceBetween: 10,
		},
		'@0.75': {
		  slidesPerView: 2.5,
		  spaceBetween: 10,
		},
		'@1.00': {
		  slidesPerView: 3.5,
		  spaceBetween: 10,
		},
		'@1.50': {
		  slidesPerView: 3.5,
		  spaceBetween: 10,
		},
	  }
	});
	
	$(document).on('click', '.toggleExpandAll', function() {
		if ($(this).html() == 'expand all'){
			$(this).html('collpase all');
			$('.401kSavingBtn').css("width", "100%");
			$('.401kSavingBtn').find("span.glyphicons").removeClass('glyphicons-plus').addClass('glyphicons-minus');
			$('#collapseSavings').collapse('show');
			$('.NonQualSavingBtn').css("width", "100%");
			$('.NonQualSavingBtn').find("span.glyphicons").removeClass('glyphicons-plus').addClass('glyphicons-minus');
			$('#collapseNonQual').collapse('show');
			$('.HSABtn').css("width", "100%");
			$('.HSABtn').find("span.glyphicons").removeClass('glyphicons-plus').addClass('glyphicons-minus');
			$('#collapseHSA').collapse('show');
			$('.basicLifeBtn').css("width", "100%");
			$('.basicLifeBtn').find("span.glyphicons").removeClass('glyphicons-plus').addClass('glyphicons-minus');
			$('#collapseBasicLife').collapse('show');
		} else {
			$(this).html('expand all');
			$('.401kSavingBtn').css("width", "300px");
			$('.401kSavingBtn').find("span.glyphicons").removeClass('glyphicons-minus').addClass('glyphicons-plus');
			$('#collapseSavings').collapse('hide');
			$('.NonQualSavingBtn').css("width", "300px");
			$('.NonQualSavingBtn').find("span.glyphicons").removeClass('glyphicons-minus').addClass('glyphicons-plus');
			$('#collapseNonQual').collapse('hide');
			$('.HSABtn').css("width", "300px");
			$('.HSABtn').find("span.glyphicons").removeClass('glyphicons-minus').addClass('glyphicons-plus');
			$('#collapseHSA').collapse('hide');
			$('.basicLifeBtn').css("width", "300px");
			$('.basicLifeBtn').find("span.glyphicons").removeClass('glyphicons-minus').addClass('glyphicons-plus');
			$('#collapseBasicLife').collapse('hide');
		}
		
	});
	

}).fail(function(error) {
     console.log('one or more scripts failed to load');
});

var swiper = new Swiper('.swiper-container', {
  slidesPerView: 1,
  spaceBetween: 10,
  // init: false,
  pagination: {
    el: '.swiper-pagination',
    clickable: true,
  },
