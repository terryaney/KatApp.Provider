$("#option2").click(function() {
	$("#contribTable").fadeOut("slow");
	
	$("#spinner").delay(300).fadeIn();
	$("#spinner").fadeOut(300);
	
	$("#sourceTable").delay(1000).fadeIn("slow");
	
});
