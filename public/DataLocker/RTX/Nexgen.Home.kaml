<script>
  (function() {
    var view = $("thisClass");

    if ( view.data("debugger" ) === true ) {
      debugger;
    }

    view.on( "onInitialized.RBLe", function( event, application ) {
      setTimeout( function() { $(".floating-arrow").fadeOut() }, 6000);
    });

    view.on( "onCalculation.RBLe", function( event, calculationResults, calcOptions, application ) {
      $(".alertMoreSuggestions")
        .off("click")
        .on("click", function() {
          if ( $(this).html() == "show less") {
            $(this).html("more suggestion(s)");
          }          
          else {
            $(this).html("show less");
          }
        });

      $("a", view).not(".anchorhook").on('click', function(e){
        var href = $(this).attr("href");
        console.log( "href " + $(this).attr("href") );
        if ( href == "#null" || href.includes("#suggestion")) {
          //hander for a elements
          //debugger;
          //e.dispatchEvent();
        }
        else {
          if ( href.includes("channel_DB")) {
            $(".lnkPension")[0].click();
          }
          else if ( href.includes("Benefits_DB") ) {
            $(".pensionStartApp").val(".katapp-sharkfin");$(".lnkPension")[0].click();
          }
          else if ( href.includes("htl_RetOnline") ) {
            $(".pensionStartApp").val(".katapp-retireonline");$(".lnkPension")[0].click();
          }
          else if ( href.includes("htl_PensionEstimate") ) {
            $(".pensionStartApp").val(".katapp-pensionestimates");$(".lnkPension")[0].click();
          }
          else if ( href.includes("Benefits_DC") ) {
            $(".pensionStartApp").val(".katapp-tr");$(".lnkPension")[0].click();
          }
          
          $("header")[0].scrollIntoView( { behavior: "smooth"});

          e.preventDefault();
          return false;
        }
      }).addClass("anchorhook");

      window.dispatchEvent(new Event('resize'));    
    });

  })();
  //# sourceURL=nexgen.home.kaml
</script>


