//ensure header included file loaded first before the containing functions can be use
$(document).ready(function () {

  $('.hero').fadeIn('slow');
  $('#carousel-example-generic').carousel({
    interval:5000,
    pause: "false"
  });
		
	$(".carouselbutton").click(function () {
		if ($(this).attr("id") === "pauseButton") {
		  $('#carousel-example-generic').carousel('pause');
				$(this).attr("id", "playButton");
		  $("span",this).toggleClass("glyphicons-play glyphicons-pause");
		} else {
				  $('#carousel-example-generic').carousel('cycle');
				$(this).attr("id", "pauseButton");
		  $("span",this).toggleClass("glyphicons-pause glyphicons-play");
		}
	});
					
	$('.carousel').carousel({
	  interval: 5000
	})

	var swiper = new Swiper('.swiper-container', {
	  slidesPerView: 1,
	  spaceBetween: 10,
	  // init: false,
	  pagination: {
		el: '.swiper-pagination',
		clickable: true,
	  },
	  breakpoints: {
		'@0.00': {
		  slidesPerView: 1.5,
		  spaceBetween: 10,
		},
		'@0.75': {
		  slidesPerView: 2.5,
		  spaceBetween: 10,
		},
		'@1.00': {
		  slidesPerView: 3.5,
		  spaceBetween: 10,
		},
		'@1.50': {
		  slidesPerView: 3.5,
		  spaceBetween: 10,
		},
	  }
	});

});

$("#member").click(function() {
	$(this).parent().siblings("#recommendedLink").removeClass("video__tab--selected");
	$(this).parent("li.video__tab").addClass("video__tab--selected");
	$(".recommended").fadeOut("slow");
	$("#spinner").delay(300).fadeIn();
	$("#spinner").fadeOut(300);
	$(".member").delay(1000).fadeIn("slow");
});
	
$("#recommended").click(function() {
	$(this).parent().siblings("#memberLink").removeClass("video__tab--selected");
	$(this).parent("li.video__tab").addClass("video__tab--selected");
	$(".member").fadeOut("slow");
	$("#spinner").delay(300).fadeIn();
	$("#spinner").fadeOut(300);
	$(".recommended").delay(1000).fadeIn("slow");
});

$('.openMoreMenuAlert').click(function() {
	$(".alertMoreMenu").fadeIn(300);
});

$('.closeMoreMenuAlert').click(function() {
	$(".alertMoreMenu").fadeOut(300);
});

</script>
